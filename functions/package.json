{
  "name": "bss-firebase-functions",
  "version": "0.1.0",
  "description": "BedStuyStrong Cloud Functions for Firebase",
  "scripts": {
    "config:get": "npx firebase functions:config:get",
    "config:test": "npx firebase use test",
    "deploy": "npx firebase deploy --only functions",
    "deploy:poll": "npx firebase deploy --only functions:poll",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "logs": "npx firebase functions:log",
    "postversion": "git push --all && git push --tags",
    "serve": "npx firebase emulators:start --only functions",
    "shell": "npx firebase functions:shell",
    "start": "npm run shell",
    "test": "mocha --reporter spec"
  },
  "engines": {
    "node": "8"
  },
  "dependencies": {
    "airtable": "^0.8.1",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.6.0",
    "libphonenumber-js": "^1.7.50",
    "lodash": "^4.17.15",
    "promise.allsettled": "^1.0.2",
    "slack": "^11.0.2",
    "twilio": "^3.42.0"
  },
  "devDependencies": {
    "@google-cloud/pubsub": "^1.7.1",
    "eslint": "^5.12.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^8.0.2",
    "mocha": "^7.1.1"
  },
  "private": true
}